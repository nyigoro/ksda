{
  "name": "ksda-songs",
  "version": "1.0.0",
  "description": "Kenyan SDA Songs Website - A community platform for Seventh-day Adventist gospel songs from Kenya",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "dev:ui": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist",
    "db:migrate": "wrangler d1 execute ksda-songs-db --file=./db/migrations/001_initial_schema.sql",
    "db:seed": "wrangler d1 execute ksda-songs-db --file=./db/seed.sql",
    "lint": "eslint .",
    "generate-types": "wrangler types",
    "type-check": "generate-types && tsc",
    "format": "prettier --write .",
    "test": "vitest --config ./frontend/vite.config.ts"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "hono": "^4.5.2",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-router-dom": "^6.8.1",
    "typescript-eslint": "^8.38.0",
    "uuid": "^11.1.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.0-beta.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.7",
    "@types/deep-eql": "^4.0.2",
    "@types/estree": "^1.0.8",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/jest": "^30.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/yargs": "^17.0.33",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.39",
    "prettier": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vitest": "^3.2.4",
    "wrangler": "^4.26.1"
  },
  "keywords": [
    "sda",
    "kenya",
    "gospel",
    "songs",
    "seventh-day-adventist",
    "worship",
    "swahili",
    "church-music"
  ],
  "author": "KSDA Songs Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nyigoro/ksda.git"
  },
  "bugs": {
    "url": "https://github.com/nyigoro/ksda/issues"
  },
  "homepage": "https://github.com/nyigoro/ksda#readme"
}
